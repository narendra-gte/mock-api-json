<p id="notice"><%= notice %></p>

<h1>FinePrints</h1>

<table>
  <thead>
    <tr>
      <th>Fine Print</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fine_print.each_with_index do |fine_print,index| %>
      <tr>
        <td><%=index+1%></td>
        <td><%= fine_print.text %></td>
      </tr>
    <% end %>
  </tbody>
</table>
 
